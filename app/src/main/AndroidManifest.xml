<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quan_ly_resort">

    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.DichVuDetailActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.RegisterActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.LoginActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.ThanhToanActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.DatPhongActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.RutTienActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.NapTienActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.StandardActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.SuperiorActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.DeluxeActivity"
            android:parentActivityName=".MainActivity" />

        <activity android:name=".activity.DetailPhongRS"
            android:label="@string/detail_phong"
            />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>